{
    "name": "hello",
    "version": "0.1.0",
    "private": true,
    "description": "S",
    "main": "main.ts",
    "scripts": {
        "build": "node scripts/build.js",
        "build-storybook": "build-storybook",
        "commit": "git-cz",
        "dev": "cross-env NODE_ENV=development nodemon --watch out --exec  './node_modules/.bin/electron' ./src/main.js",
        "start": "node scripts/start.js",
        "test": "node scripts/test.js --env=jsdom",
        "test:coverage": "node scripts/test.js --env=jsdom --coverage",
        "tools": "electron ./node_modules/electron-toolkit"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "babel": {
        "plugins": [
            "syntax-dynamic-import"
        ],
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "D:\\Code\\my-app\\tsconfig.test.json"
            }
        },
        "moduleFileExtensions": [
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node",
            "mjs"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
        ],
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
        ]
    },
    "dependencies": {
        "autoprefixer": "^9.1.5",
        "axios": "^0.18.0",
        "chalk": "^2.4.1",
        "classnames": "^2.2.6",
        "css-loader": "^1.0.0",
        "dotenv": "^6.0.0",
        "electron": "^3.0.3",
        "file-loader": "^2.0.0",
        "fs-extra": "^7.0.0",
        "mini-css-extract-plugin": "0.4.3",
        "mobx": "^5.5.0",
        "mobx-react": "^5.2.8",
        "mobx-react-router": "^4.0.5",
        "object-assign": "4.1.1",
        "promise": "^8.0.2",
        "raf": "3.4.0",
        "react": "^16.5.2",
        "react-app-polyfill": "^0.1.3",
        "react-dev-utils": "^6.0.4",
        "react-dom": "^16.5.2",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-transition-group": "^2.5.0",
        "resolve": "1.8.1",
        "ts-node": "^7.0.1",
        "whatwg-fetch": "^3.0.0",
        "workbox-webpack-plugin": "3.6.2"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.14",
        "@types/jest": "^23.3.2",
        "@types/node": "^10.11.0",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.7",
        "@types/react-router": "^4.0.31",
        "@types/react-router-dom": "^4.3.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.4",
        "babel-plugin-named-asset-import": "^0.2.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-react-app": "^5.0.3",
        "babel-runtime": "^6.26.0",
        "bfj": "6.1.1",
        "case-sensitive-paths-webpack-plugin": "^2.1.2",
        "commitizen": "^3.0.2",
        "cross-env": "^5.2.0",
        "cz-conventional-changelog": "^2.1.0",
        "electron-builder": "^20.28.4",
        "electron-toolkit": "^1.0.24",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "eslint": "^5.6.1",
        "eslint-config-react-app": "^3.0.3",
        "eslint-loader": "2.1.1",
        "eslint-plugin-flowtype": "^2.50.3",
        "eslint-plugin-import": "2.14.0",
