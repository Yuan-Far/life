        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "nodemon": "^1.18.4",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "pnp-webpack-plugin": "1.1.0",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "^6.0.10",
        "postcss-safe-parser": "4.0.1",
        "react-loadable": "^5.5.0",
        "react-test-renderer": "^16.5.2",
        "sass-loader": "7.1.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.0",
        "sw-precache-webpack-plugin": "^0.11.5",
        "terser-webpack-plugin": "1.1.0",
        "ts-jest": "^23.10.4",
        "ts-loader": "^5.2.1",
        "tslint": "^5.11.0",
        "tslint-loader": "^3.6.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.1.1",
        "url-loader": "^1.1.1",
        "webpack": "^4.20.2",
        "webpack-dev-server": "3.1.9",
        "webpack-manifest-plugin": "2.0.4"
    },
    "engines": {
        "node": ">=7.6.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "build": {
        "appId": "com.electron.hello",
        "copyright": "Copyright Â© 2018 undefined",
        "productName": "hello",
        "electronVersion": "3.0.3",
        "asar": false,
        "directories": {
            "app": "D:\\Code\\IM",
            "output": "dist"
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "arch_all"
                    ]
                }
            ]
        }
    }
}
